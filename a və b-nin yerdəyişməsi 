using System;

namespace introduction 
{
    class Program
    {
        static void Main(string[] args)
        {
            int a = 6;
            int b = 9;
            Console.WriteLine("a="+a);
            Console.WriteLine("b="+b);
            a = a + b;
            b = a - b;
            a = a - b;
            Console.WriteLine("a=" + a + " " + "b=" + b);
                

        }
    }
}
